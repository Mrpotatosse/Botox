<UserControl x:Class="BotoxUI.Views.CustomButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BotoxUI.Views"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="150">
    <DockPanel LastChildFill="True">
        <DockPanel Name="IsSelectedDock" DockPanel.Dock="Left" Width="10" Background="#36B50E"></DockPanel>
        <TextBlock Name="ContentText" TextAlignment="Center" VerticalAlignment="Center" >TEXT</TextBlock>
    </DockPanel>
</UserControl>
